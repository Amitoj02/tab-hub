{
    "manifest_version": 3,
    "name": "Tabhub - Github Dashboard",
    "version": "1.0.0",
    "description": "Change the new tab with Github dashboard to preview issues and pull requests",
    "icons": {
        "48": "assets/images/logo-48.png",
        "128": "assets/images/logo-128.png"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "storage"
    ]
}
